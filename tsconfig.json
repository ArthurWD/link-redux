{
  "compilerOptions": {
    "allowJs": false,
    "declaration": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react",
    "lib": [
      "es2015",
      "es2016",
      "es2017",
      "dom"
    ],
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "outDir": "dist/es",
    "sourceMap": true,
    "strict": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types",
      "./node_modules/link-lib/@types",
      "./node_modules/jest-enzyme/lib"
    ]
  },
  "files": [
    "src/link-redux.ts"
  ],
  "exclude": [
    "**/__tests__/*"
  ]
}
