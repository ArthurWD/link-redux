{
  "name": "link-redux",
  "version": "1.0.0-beta28.35",
  "description": "React/Redux extensions for the Link library",
  "main": "dist/link-redux.umd.js",
  "module": "dist/link-redux.es6.js",
  "sideEffects": false,
  "typings": "dist/typings/link-redux.d.ts",
  "files": [
    "@types",
    "dist/link-redux.es6.js",
    "dist/link-redux.umd.js",
    "dist/typings/**/*.*",
    "package.json",
    "tsconfig.json",
    "yarn.lock"
  ],
  "repository": "https://github.com/fletcher91/link-redux.git",
  "scripts": {
    "build": "rollup -c && tsc -p tsconfig-types.json",
    "doc": "rm -rf dist/docs/* && typedoc --tsconfig tsconfig-types.json --exclude **/*.spec.ts --excludeExternals --excludePrivate --mode file --theme minimal --target ES6 --includeDeclarations --out dist/docs/ src/",
    "doc:deploy": "gh-pages -d ./dist/docs",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prebuild": "rm -rf dist/",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "test": "jest --coverage"
  },
  "author": "Fletcher91 <thom@argu.co>",
  "license": "LGPL-3.0",
  "dependencies": {
    "es6-promise": "^4.2.4",
    "http-status-codes": "^1.3.0",
    "immutable": "^3.8.2",
    "link-lib": "=1.0.0-beta21.2",
    "prop-types": "15.6.2",
    "rdflib": "^0.17.0",
    "react": "^16.4.1",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-batched-actions": "^0.3.0"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.11",
    "@types/jest": "^22.2.3",
    "@types/prop-types": "^15.5.3",
    "@types/react": "=16.4.6",
    "@types/react-redux": "^6.0.3",
    "@types/redux-actions": "^2.3.0",
    "@types/redux-immutable": "^3.0.38",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "npm:enzyme-react-adapter-future",
    "gh-pages": "^1.2.0",
    "jest": "^22.4.4",
    "jest-enzyme": "^6.0.2",
    "react-dom": "^16.4.1",
    "react-test-renderer": "^16.4.1",
    "redux-actions": "^2.4.0",
    "redux-immutable": "^4.0.0",
    "rollup": "^0.62.0",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.15.1",
    "ts-jest": "^23.0.0",
    "tslint": "^5.10.0",
    "tslint-react": "^3.6.0",
    "type-zoo": "^3.0.0",
    "typedoc": "^0.11.1",
    "typescript": "2.9.2",
    "whatwg-fetch": "^2.0.4",
    "whatwg-url": "^6.5.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": false
      }
    },
    "testMatch": [
      "**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "testURL": "http://example.org/resources/5",
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
