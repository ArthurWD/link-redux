{
  "name": "link-redux",
  "version": "1.0.0-beta17",
  "description": "React/Redux extensions for the Link library",
  "main": "dist/link-redux.umd.js",
  "module": "dist/link-redux.es6.js",
  "typings": "dist/typings/link-redux.d.ts",
  "files": [
    "@types",
    "dist/link-redux.es6.js",
    "dist/link-redux.umd.js",
    "dist/typings/**/*.*"
  ],
  "repository": "https://github.com/fletcher91/link-redux.git",
  "scripts": {
    "build": "rollup -c && tsc -p tsconfig-types.json",
    "doc": "rm -rf dist/docs/* && typedoc --tsconfig tsconfig-types.json --exclude **/*.spec.ts --excludeExternals --excludePrivate --mode file --theme minimal --target ES6 --includeDeclarations --out dist/docs/ src/",
    "doc:deploy": "gh-pages -d ./dist/docs",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prebuild": "rm -rf dist/",
    "prepublish": "npm run build && npm run doc",
    "postpublish": "npm run doc:deploy",
    "pretest": "npm run lint",
    "test": "jest --coverage"
  },
  "author": "Fletcher91 <thom@argu.co>",
  "license": "LGPL-3.0",
  "dependencies": {
    "es6-promise": "^4.2.2",
    "http-status-codes": "^1.3.0",
    "immutable": "^3.8.1",
    "jsonld": "^0.4.11",
    "link-lib": "^1.0.0-beta11",
    "prop-types": "15.6.0",
    "rdflib": "^0.16.5",
    "react": "^16.2.0",
    "react-redux": "^5.0.6",
    "redux": "^4.0.0-beta.1",
    "redux-batched-actions": "^0.2.1"
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.8",
    "@types/jest": "^22.1.1",
    "@types/prop-types": "^15.5.2",
    "@types/react": "^16.0.35",
    "@types/react-redux": "^5.0.14",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^2.2.3",
    "@types/redux-immutable": "^3.0.38",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gh-pages": "^1.1.0",
    "jest": "^22.1.4",
    "jest-enzyme": "^4.1.1",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "redux-actions": "^2.2.1",
    "redux-immutable": "^4.0.0",
    "rollup": "^0.55.1",
    "rollup-plugin-commonjs": "^8.3.0",
    "rollup-plugin-node-resolve": "^3.0.2",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.10.0",
    "ts-jest": "^22.0.3",
    "tslint": "^5.9.1",
    "typedoc": "^0.10.0",
    "typescript": "2.7.1",
    "whatwg-fetch": "^2.0.3",
    "whatwg-url": "^6.4.0"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "enableTsDiagnostics": false
      }
    },
    "testMatch": [
      "**/*.spec.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "testURL": "http://example.org/resources/5",
    "transform": {
      "\\.ts$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  }
}
